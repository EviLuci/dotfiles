    // Group Configuration
    "group/power": {
      "orientation": "inherit",
      "drawer": {
        "transition-duration": 500,
        "children-class": "drawer-child",
        "transition-left-to-right": false
      },
      "modules": [
        "custom/power", // First element is the "group leader" and won't ever be hidden
        "custom/quit",
        "custom/lock",
        "custom/reboot"
      ]
    },
    "custom/quit": {
      "format": "ó°—¼",
      "tooltip": false,
      "on-click": "hyprctl dispatch exit"
    },
    "custom/lock": {
      "format": "ó°",
      "tooltip": false,
      "on-click": "hyprlock"
    },
    "custom/reboot": {
      "format": "ó°œ‰",
      "tooltip": false,
      "on-click": "reboot"
    },
    "custom/power": {
      "format": "ï€‘",
      "tooltip": false,
      "on-click": "shutdown now"
    },
    "group/cpu-load": {
