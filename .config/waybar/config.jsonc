    "name": "topbar",
    "layer": "top",
    "position": "top",
    "mode": "dock",
    "exclusive": true,
    "spacing": 2,
    "passthrough": false,
    "gtk-layer-shell": true,
    "reload_style_on_change": true,
    "modules-left": [
      "custom/updates",
      "keyboard-state",
      "clock",
      "bluetooth",
      "network",
      "privacy",
      "hyprland/submap"
    ],
    "modules-center": ["hyprland/window"],
    "modules-right": [
      "mpris",
      "mpd",
      "battery",
      "backlight",
      "wireplumber",
      "temperature",
      "group/cpu-load",
      "memory",
