[
  {
    "name": "topbar",
    "layer": "top",
    "position": "top",
    "height": 30,
    "mode": "dock",
    "exclusive": true,
    "spacing": 0,
    "passthrough": false,
    "gtk-layer-shell": true,
    "reload_style_on_change": true,
    "modules-left": [
      "custom/updates",
      "clock",
      "bluetooth",
      "network",
      "privacy",
      "hyprland/submap"
    ],
    "modules-center": [
      "hyprland/window"
    ],
    "modules-right": [
      "mpris",
      "battery",
      "backlight",
      "wireplumber",
      "temperature",
      "group/cpu-load",
      "memory",
      "idle_inhibitor",
      "systemd-failed-units",
      "group/power"
    ],
    // Group Configuration
    "group/power": {
      "orientation": "inherit",
      "drawer": {
        "transition-duration": 500,
        "children-class": "power-child",
        "transition-left-to-right": false
      },
      "modules": [
        "custom/power", // First element is the "group leader" and won't ever be hidden
        "custom/quit",
        "custom/lock",
        "custom/reboot"
      ]
    },
    "custom/quit": {
      "format": "Û∞óº",
      "tooltip": true,
      "tooltip-format": "Quit",
      "on-click": "hyprctl dispatch exit"
    },
    "custom/lock": {
      "format": "Û∞çÅ",
      "tooltip": true,
      "tooltip-format": "Lock",
      "on-click": "hyprlock"
    },
    "custom/reboot": {
      "format": "Û∞úâ",
      "tooltip": true,
      "tooltip-format": "Reboot",
      "on-click": "reboot"
    },
    "custom/power": {
      "format": "ÔÄë",
      "tooltip": true,
      "tooltip-format": "Shutdown",
      "on-click": "shutdown now"
    },
    "group/cpu-load": {
      "orientation": "inherit",
      "drawer": {
        "transition-duration": 500,
        "children-class": "cpu-load-child",
        "transition-left-to-right": false
      },
      "modules": [
        "cpu",
        "load"
      ]
    },
    "cpu": {
      "interval": 1,
      "format": "Û∞çõ {usage}%",
      "on-click": "",
      "tooltip": false
    },
    "load": {
      "interval": 10,
      "format": "load: {load1} {load5} {load15}"
    },
    // Modules configuration
    "backlight": {
      "device": "intel_backlight",
      "format": "{icon} {percent}%",
      "format-icons": [
        "Óéç",
        "Óèì",
        "Óèë",
        "Óèè",
        "Óèé",
        "Óèç",
        "Óèä",
        "Óèà",
        "Óéõ"
      ],
      "on-scroll-up": "brightnessctl set 1%+",
      "on-scroll-down": "brightnessctl set 1%-"
    },
    "memory": {
      "interval": 30,
      "format": "Óâ¶ {}%",
      "format-alt": "Ôë≤ {used}G",
      "tooltip": true,
      "tooltip-format": "{used:0.1f}G/{total:0.1f}G"
    },
    "bluetooth": {
      "format": "Ôäì {status}",
      "format-connected": "Ôäì {device_alias}",
      "format-connected-battery": "Ôäì {device_alias} {device_battery_percentage}%",
      // "format-device-preference": [ "device1", "device2" ], // preference list deciding the displayed device
      "tooltip-format": "{controller_alias}\t{controller_address}\n\n{num_connections} connected",
      "tooltip-format-connected": "{controller_alias}\t{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
      "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
      "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%"
    },
    "temperature": {
      // "thermal-zone": 2,
      // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
      "critical-threshold": 80,
      // "format-critical": "{temperatureC}¬∞C {icon}",
      "format": "{icon} {temperatureC}¬∞C",
      "format-alt": "{temperatureF}¬∞F {icon}",
      "format-icons": [
        "Ôù´",
        "Ôãâ",
        "Ôù©"
      ],
      "tooltip": false
    },
    "hyprland/window": {
      "format": "{}",
      "max-length": 50,
      "rewrite": {
        "(.*) ‚Äî Mozilla Firefox": "üåé $1"
      },
      "separate-outputs": true
    },
    "privacy": {
      "icon-spacing": 4,
      "icon-size": 14,
      "transition-duration": 250,
      "modules": [
        {
          "type": "screenshare",
          "tooltip": true,
          "tooltip-icon-size": 14
        },
        {
          "type": "audio-out",
          "tooltip": true,
          "tooltip-icon-size": 14
        },
        {
          "type": "audio-in",
          "tooltip": true,
          "tooltip-icon-size": 14
        }
      ]
    },
    "network": {
      //"interface": "wlp2s0",
      "format": "{ifname} [Û∞æÜ {bandwidthTotalBytes}]",
      "format-wifi": "{icon} {essid} [Û∞æÜ {bandwidthTotalBytes}]",
      "format-ethernet": "Û±òñ  {ifname} [Û∞æÜ {bandwidthTotalBytes}]",
      "format-disconnected": "Û∞åô Disconnected ‚ö†",
      "format-alt": "Ó¨Å  {ipaddr} ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
      "tooltip-format": "{ifname} via {gwaddr} Ôûñ",
      "tooltip-format-wifi": "{frequency} MHz ({signalStrength}%)",
      "tooltip-format-ethernet": "{ipaddr}/{cidr} ÔÑπ {bandwidthUpBytes} ÔÑ∫ {bandwidthDownBytes}",
      "tooltip-format-disconnected": "Disconnected ‚ö†",
      "max-length": 50,
      "format-icons": [
        "Û∞§Ø ",
        "Û∞§ü ",
        "Û∞§¢ ",
        "Û∞§¢ ",
        "Û∞§® "
      ]
    },
    "clock": {
      "format": "‚è∞{:%I:%M %p}",
      "format-alt": "üìÖ{:%A, %B %d, %Y (%I:%M %p)}",
      //"format": "<span color='#bf616a'>ÔÄó </span>{:%I:%M %p}",
      //"format-alt": "<span color='#bf616a'>ÔÅ≥ </span>{:%A, %B %d, %Y (%I:%M %p)}",
      // "format": "{:ÔÄó %R  ÔÅ≥ %d/%m}",
      "tooltip-format": "<tt><small>{calendar}</small></tt>",
      "calendar": {
        "mode": "year",
        "mode-mon-col": 3,
        "weeks-pos": "right",
        "on-scroll": 1,
        "on-click-right": "mode",
        "format": {
